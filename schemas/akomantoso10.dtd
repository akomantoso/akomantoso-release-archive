<!ELEMENT clauses     (%hierElements;)+>

<!ATTLIST clauses %coreopt;  >
<!ELEMENT debate      (subdivision+)>

<!ATTLIST debate %coreopt;  >
<!ELEMENT maincontent (%hierElements; | subdivision | %blockElements; | %containerElements;)+>

<!ATTLIST maincontent %coreopt;  >

